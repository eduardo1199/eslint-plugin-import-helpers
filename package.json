{
	"name": "eslint-plugin-import-helpers",
	"version": "1.2.1",
	"description": "ESLint Rules to Aid with Imports",
	"main": "lib/index.js",
	"scripts": {
		"build": "gulp src",
		"lint": "npm run lint:js",
		"lint:js": "eslint --cache .",
		"prepublish": "npm run build",
		"test": "npm run build && npm run test-quick",
		"test-quick": "NODE_PATH=./lib nyc -s mocha -R dot --recursive test -t test-results"
	},
	"keywords": [
		"eslint",
		"eslint-plugin",
		"eslintplugin",
		"import",
		"eslint-plugin-import",
		"configurable"
	],
	"repository": {
		"url": "https://github.com/Tibfib/eslint-plugin-import-helpers.git"
	},
	"author": "Will Honey",
	"license": "MIT",
	"peerDependencies": {
		"eslint": "5.x - 8.x"
	},
	"devDependencies": {
		"@types/node": "^14.17.33",
		"@typescript-eslint/eslint-plugin": "^5.9.0",
		"@typescript-eslint/parser": "^5.3.1",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-eslint-plugin": "^4.1.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gulp": "^4.0.2",
		"gulp-typescript": "^5.0.1",
		"mocha": "^6.1.2",
		"nyc": "^13.3.0",
		"prettier": "^2.5.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.4.4"
	},
	"dependencies": {},
	"resolutions": {
		"graceful-fs": "4.2.3"
	}
}
